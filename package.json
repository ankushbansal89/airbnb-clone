{
  "name": "airbnb-clone",
  "version": "1.0.0",
  "description": "This is the clone of airbnb using react, graphql, serverless etc",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "./packages/**"
  ],
  "scripts": {
    "bootstrap": "lerna clean -y && lerna bootstrap --hoist",
    "format": "prettier \"./packages/**/src/*.{js,tsx,ts,html}\" --write",
    "lint": "eslint \"./packages/**/src/*.{js,jsx,ts,tsx}\"",
    "test": "lerna run test --stream",
    "build": "lerna run build --stream",
    "start": "lerna run start --stream",
    "deploy": "lerna run deploy --stream",
    "prepare": "husky install",
    "ts-coverage": "typescript-coverage-report"
  },
  "keywords": [
    "React",
    "graphql",
    "apollo",
    "serverless",
    "airbnb"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-lambda": "^2.16.1",
    "graphql": "^15.3.0",
    "mongodb-client-encryption": "^1.2.6",
    "mongoose": "^5.12.5",
    "serverless-bundle": "^4.2.2",
    "serverless-dotenv-plugin": "^3.9.0",
    "serverless-offline": "^6.9.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@types/graphql": "^14.5.0",
    "@types/mongoose": "^5.10.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.20.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "apollo-server-types": "^0.7.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^5.1.3",
    "lerna": "^4.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3",
    "typescript-coverage-report": "^0.5.1"
  }
}
